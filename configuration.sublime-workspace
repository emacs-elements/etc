n-p) ; Check if a region is selected\n    (let ((start (region-beginning)) ; Get the starting point of the region\n\t  (end (region-end)) ; Get the ending point of the region\n\t  (pattern \"^[A-Z].*[0-9]+\\s+\")) ; The regex pattern to match\n      (let ((modified-text (replace-regexp-in-string\n\t\t\t    pattern \"\" (buffer-substring-no-properties start end)))) ; Get the modified text\n\t(delete-region start end) ; Delete the original region\n\t(goto-char start) ; Move the cursor to the start of the original region\n\t(insert modified-text))))) ; Insert the modified text\n\n;; Delete a sentence + the extra space afterwards\n\n(defun kill-sentence-and-trailing-space ()\n  \"Kill sentence and any trailing whitespace.\"\n  (interactive)\n  (kill-sentence)\n  (while (looking-at-p \"[ \\t\\n]\")\n    (delete-char 1)))\n\n;; Add pairs\n\n(defun my-add-asterisks (beg end)\n  \"Add asterisks around a region of text.\"\n  (interactive \"r\")\n  (save-excursion\n\t(goto-char end) ; move to end of region\n\t(insert \"*\") ; insert asterisk\n\t(goto-char beg) ; move to beginning of region\n\t(insert \"*\"))) ; insert asterisk\n\n(defun my-add-equals (beg end)\n  \"Add tildes around a region of text.\"\n  (interactive \"r\")\n  (save-excursion\n\t(goto-char end)\n\t(insert \"=\")\n\t(goto-char beg)\n\t(insert \"=\")))\n\n(defun my-add-quotes (beg end)\n  \"Add quotes around a region of text.\"\n  (interactive \"r\")\n  (save-excursion\n\t(goto-char end)\n\t(insert \"\\\"\")\n\t(goto-char beg)\n\t(insert \"\\\"\")))\n\n(defun jump-out-of-pair ()\n\t(interactive)\n\t(let ((found (search-forward-regexp \"[])}\\\"'`*=]\" nil t)))\n\t\t(when found\n\t\t\t(cond ((or (looking-back \"\\\\*\\\\*\" 2)\n\t\t (looking-back \"``\" 2)\n\t\t (looking-back \"''\" 2)\n\t\t (looking-back \"==\" 2))\n\t\t\t (forward-char))\n\t\t\t      (t (forward-char 0))))))\n\n;; Duplicate a line in Emacs\n\n(defun duplicate-line ()\n  \"Duplicate the current line without affecting the kill ring.\"\n  (interactive)\n  (let ((current-kill (current-kill 0 t))  ; Get the current kill content without rotating the ring.\n\t(begin (line-beginning-position))\n\t(end (line-end-position)))\n    ;; Duplicate the line\n    (goto-char end)\n    (insert \"\\n\" (buffer-substring begin end))\n    ;; Restore the original kill content\n    (kill-new current-kill t)))\n\n(defun move-line-up ()\n\t\"Move the current line up.\"\n\t(interactive)\n\t(transpose-lines 1)\n\t(forward-line -2))\n\n(defun move-line-down ()\n\t\"Move the current line down.\"\n\t(interactive)\n\t(forward-line 1)\n\t(transpose-lines 1)\n\t(forward-line -1))\n\n\f\n\n(require 'langtool)\n(setq langtool-default-language \"en-US\")\n(setq langtool-language-tool-jar \"~/.emacs.d/LanguageTool-6.3-SNAPSHOT/languagetool-commandline.jar\")\n(setq langtool-java-bin \"/usr/bin/java\")\n(setq langtool-java-user-arguments '(\"-Dfile.encoding=UTF-8\"))\n\n\f\n\n(defun search-contacts (regex)\n  (interactive \"sEnter the regex pattern to search contacts: \")\n  (let ((text-file \"/home/tan/People/contacts.txt\")\n\t(lines nil))\n    (with-temp-buffer\n      (insert-file-contents text-file)\n      (goto-char (point-min))\n      (while (not (eobp))\n\t(let ((line (buffer-substring-no-properties (line-beginning-position) (line-end-position))))\n\t  (when (string-match-p regex line)\n\t    (push line lines)))\n\t(forward-line 1)))\n    (dolist (line (nreverse lines))\n      (insert line \"\\n\"))))\n\n(require 'hydra)\n\n(defun my-defshortcut (key command &optional docstring)\n  \"Define a key shortcut with optional documentation.\"\n  (let ((fn (intern (concat \"my/shortcut-\" (symbol-name key)))))\n    (eval `(defun ,fn ()\n\t     ,(if docstring docstring \"\")\n\t     (interactive)\n\t     ,command)))\n  (global-set-key (kbd (concat \"C-c \" (string key))) (intern (concat \"my/shortcut-\" (symbol-name key)))))\n\n;; (use-package hydra\n;;   :config\n;;   (defhydra my-file-shortcuts ()\n;;     \"File shortcuts\"\n;;     (\"B\" (find-file \"~/bin\") \"bin\")\n;;     (\"a\" (find-file \"/home/tan/org-agenda/agenda.org\") \"agenda\")\n;;     (\"b\" (find-file \"~/.bashrc\") \"bashrc\")\n;;     (\"e\" (find-file \"~/Elements\") \"Elements\")\n;;     (\"f\" (find-file \"~/Files\") \"Files\")\n;;     (\"n\" (find-file \"~/Notes\") \"Notes\"))\n;;   :bind\n;;   (\"C-c f\" . my-file-shortcuts/body))\n\n(use-package hydra\n  :config\n  (defhydra my-file-shortcuts ()\n    \"File shortcuts\"\n    (\"B\" (find-file \"~/bin\") \"bin\" :exit t)\n    (\"a\" (find-file \"/home/tan/org-agenda/agenda.org\") \"agenda\" :exit t)\n    (\"b\" (find-file \"~/.bashrc\") \"bashrc\" :exit t)\n    (\"e\" (find-file \"~/Elements\") \"Elements\" :exit t)\n    (\"f\" (find-file \"~/Files\") \"Files\" :exit t)\n    (\"n\" (find-file \"~/Notes\") \"Notes\" :exit t))\n  :bind\n  (\"C-c f\" . my-file-shortcuts/body))\n\n\f\n\n(require 'find-dired)\n(setq find-ls-option '(\"-print0 | xargs -0 ls -ld\" . \"-ld\"))\n\n\f\n\n;; (setq sentence-end \"[.\\\".,;!?*:'\\\"] \")\n\n(defvar reading-aloud-p nil\n  \"State variable to track if reading aloud is active.\")\n\n(defun toggle-read-aloud ()\n  \"Toggle between reading aloud and stopping based on state.\"\n  (interactive)\n  (if reading-aloud-p\n      (progn\n\t(stop-reading-aloud)\n\t(setq reading-aloud-p nil))\n    (progn\n      (read-aloud-from-cursor)\n      (setq reading-aloud-p t))))\n\n\f\n\n(with-eval-after-load 'org\n  (define-key org-mode-map (kbd \"C-v\") nil))\n\n(with-eval-after-load 'org\n  (define-key org-mode-map (kbd \"M-v\") nil))\n\n\f\n\n(defun er-copy-file-name-to-clipboard ()\n  \"Copy the current buffer file name to the clipboard.\"\n  (interactive)\n  (let ((filename (if (equal major-mode 'dired-mode)\n\t\t      default-directory\n\t\t    (buffer-file-name))))\n    (when filename\n      (kill-new filename)\n      (message \"Copied buffer file name '%s' to the clipboard.\" filename))))\n\n\f\n\n(use-package link-hint\n  :bind\n  (\"M-g u\" . link-hint-ope