    ;; until `bob'.\n\t       (not (bobp)))\n\t(backward-word)\n\t(backward-char))\n      (setq aft (endless/simple-get-word)))\n    (if (and aft bef (not (equal aft bef)))\n\t(let ((aft (downcase aft))\n\t      (bef (downcase bef)))\n\t  (define-abbrev\n\t    (if p local-abbrev-table global-abbrev-table)\n\t    bef aft)\n\t  (message \"\\\"%s\\\" now expands to \\\"%s\\\" %sally\"\n\t\t   bef aft (if p \"loc\" \"glob\")))\n      (user-error \"No typo at or before point\"))))\n\n(setq save-abbrevs 'silently)\n(setq-default abbrev-mode t)\n\n\f\n\n(setq org-export-preserve-breaks t)\n\n\f\n\n;; Suppress end of buffer warning\n\n(defadvice previous-line (around silencer activate)\n  (condition-case nil\n      ad-do-it\n    (beginning-of-buffer nil)))\n\n(defadvice next-line (around silencer activate)\n  (condition-case nil\n      ad-do-it\n    (end-of-buffer nil)))\n\n\f\n\n(setq-default fill-column 90)\n\n\f\n\n;; Multiple cursors\n\n(require 'multiple-cursors)\n\n(define-key input-decode-map [?\\C-m] [C-m])\n\n(use-package multiple-cursors\n  :demand t\n  :bind ((\"<C-m> b\"     . mc/edit-beginnings-of-lines)\n\t (\"<C-m> '\"     . mc/edit-ends-of-lines)\n\t (\"<C-m> n\"     . mc/mark-next-like-this)\n\t (\"<C-m> p\"     . mc/mark-previous-like-this)\n\t (\"<C-m> N\"     . mc/skip-to-next-like-this)\n\t (\"<C-m> P\"     . mc/skip-to-previous-like-this)\n\t (\"<C-m> W\"     . mc/mark-all-words-like-this)\n\t (\"<C-m> a\"     . mc/mark-all-like-this)\n\t (\"<C-m> d\"     . mc/mark-all-like-this-dwim)\n\t (\"<C-m> l\"     . mc/edit-lines)\n\t (\"M-S-<down>\" . mc/mark-next-like-this)\n\t (\"M-S-<up>\" . mc/mark-previous-like-this)\n\t (\"M-S-<right>\" . mc/skip-to-next-like-this)\n\t (\"M-S-<left>\" . mc/skip-to-previous-like-this)))\n\n(defun my-mc-setup ()\n  (define-key mc/keymap (kbd \"<return>\") nil))\n\n(add-hook 'multiple-cursors-mode-hook 'my-mc-setup)\n\n\f\n\n(add-hook 'dired-mode-hook 'dired-hide-details-mode)\n(add-hook 'dired-mode-hook 'dired-omit-mode)\n