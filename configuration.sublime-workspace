TINGS\n\n;; Plan 9 Smart Shell\n\n(require 'em-smart)\n(setq eshell-where-to-jump 'begin)\n(setq eshell-review-quick-commands nil)\n(setq eshell-smart-space-goes-to-end t)\n\n;; If the above does not work, try uncommenting this.\n(add-to-list 'eshell-modules-list 'eshell-smart)\n\n;; Suppress the startup banner message\n(setq eshell-banner-message \"\")\n\n;; Set the Eshell history size\n(setq eshell-history-size 500)\n\n;; Save history when exiting\n(setq eshell-save-history-on-exit t)\n\n;; Ignore duplicate commands in history\n(setq eshell-hist-ignoredups t)\n\n;; Controls the size of the directory stack\n(setq eshell-last-dir-ring-size 500)\n\n;; Set the Eshell prompt\n(setq eshell-prompt-regexp \"^[^$\\n]* [#$] \")\n(setq eshell-modules-list\n      '(eshell-alias eshell-banner eshell-basic eshell-cmpl eshell-dirs\n\t\t     eshell-extpipe eshell-glob eshell-hist eshell-ls\n\t\t     eshell-pred eshell-prompt eshell-script eshell-smart\n\t\t     eshell-term eshell-unix))\n\n;; Show the five most recent files in any directory. Equivalent command to `ls -UFth --color=auto | head -3'\n\n(defun eshell/ls-recent (&rest args)\n  \"List N most recent files in current directory.\"\n  (let* ((n (if args (string-to-number (car args)) 5))  ; default to 5 files if no args\n\t (all-files (directory-files \".\" nil nil t))\n\t (files (cl-remove-if #'file-directory-p all-files))\n\t (sorted-files (sort files (lambda (a b) (time-less-p (nth 5 (file-attributes a))\n\t\t\t\t\t\t\t      (nth 5 (file-attributes b))))))\n\t (top-files (last sorted-files n)))\n    (apply 'eshell/ls \"-l\" top-files)))\n\n;; Convert docx to utf-8 text in eshell\n\n(defun eshell/docx2txt (filename)\n  (let ((output-file (concat (file-name-sans-extension filename) \".txt\")))\n    (eshell-command-result (format \"pandoc -s %s -o %s\" filename output-file))))\n\n;; ls default switches\n\n(setq eshell-ls-initial-args '(\"-1a\"))\n\n(defalias 'ls10 'eshell/ls10)\n\n;; Do automatic ls after `cd'\n\n;; (setq eshell-list-files-after-cd t)\n\n;; Additional package: Toggle eshell window\n\n;; Update eshell alias file after save\n\n(defun reload-eshell-aliases ()\n  (when (string= (buffer-file-name) \"c:/tan/.emacs.d/eshell/alias\")\n    (eshell-read-aliases-list)\n    (message \"Reloaded Eshell aliases\")))\n(add-hook 'after-save-hook 'reload-eshell-aliases)\n\n;; Permanent aliases\n\n(add-hook 'eshell-mode-hook (lambda () (eshell/alias \"911\" \"grep -ri $1 c:/tan/contacts.txt\")))\n(add-hook 'eshell-mode-hook (lambda () (eshell/alias \"999\" \"grep -ri $1 c:/tan/People/*txt\")))\n(add-hook 'eshell-mode-hook (lambda () (eshell/alias \"agi\" \"ag -i $1 .\")))\n\n;; Start Emacs in eshell\n\n;; (add-hook 'emacs-startup-hook 'eshell)\n\n;; Make sure readlink shows paths as Windows does in eshell: c:/ not /c/\n\n(defun convert-to-windows-path (path)\n  \"Convert a MSYS2-style PATH to a native Windows path.\"\n  (if (string-match \"^/\\\\([a-zA-Z]\\\\)/\\\\(.*\\\\)\" path)\n      (concat (match-string 1 path) \":/\" (match-string 2 path))\n    path))\n\n(defun eshell/readlink (&rest args)\n  (let ((result (shell-command-to-string (concat \"readlink \" (mapconcat 'identity args \" \")))))\n    (convert-to-windows-path (string-trim result))))\n\n;; When in eshell \"Home\" takes me to the beginning of the prompt, not the line\n\n(defun eshell-beginning-of-prompt ()\n  \"Move cursor to the beginning of the prompt.\"\n  (interactive)\n  (let ((prompt-start (save-excursion (eshell-bol) (point))))\n    (if (>= (point) prompt-start)\n\t(goto-char prompt-start)\n      (eshell-bol))))\n\n(add-hook 'eshell-mode-hook\n\t  (lambda ()\n\t    (local-set-key (kbd \"<home>\") 'eshell-beginning-of-prompt)))\n\n;; View multiple files in view-mode in eshell\n;; URL: https://howardism.org/Technical/Emacs/eshell-why.html\n\n(defun eshell-fn-on-files (fun1 fun2 args)\n  \"Call FUN1 on the first element in list, ARGS.\nCall FUN2 on all the rest of the elements in ARGS.\"\n  (unless (null args)\n    (let ((filenames (flatten-list args)))\n      (funcall fun1 (car filenames))\n      (when (cdr filenames)\n\t(mapc fun2 (cdr filenames))))\n    ;; Return an empty string, as the return value from `fun1'\n    ;; probably isn't helpful to display in the `eshell' window.\n    \"\"))\n\n(defun eshell/less (&rest files)\n  \"Essentially an alias to the `view-file' function.\"\n  (eshell-fn-on-files 'view-file 'view-file-other-window files))\n\n(defalias 'eshell/more 'eshell/less)\n\n;; Clear eshell\n;; URL: https://www.n16f.net/blog/clearing-the-eshell-buffer/\n\n(defun g-eshell-clear ()\n  (interactive)\n  (let ((input (eshell-get-old-input)))\n    (eshell/clear t)\n    (eshell-emit-prompt)\n    (insert input)))\n\n(defun g-eshell-init-keys ()\n  (define-key eshell-mode-map (kbd \"C-l\") 'g-eshell-clear))\n\n(add-hook 'eshell-mode-hook 'g-eshell-init-keys)\n\n;; More tweaked variables\n\n(setq eshell-rm-removes-directories t)\n(setq eshell-visual-options '((\"(\\\"git\\\" \\\"--help\\\" \\\"--paginate\\\")\")))\n(setq eshell-visual-subcommands '((\"(\\\"git\\\" \\\"log\\\" \\\"diff\\\" \\\"show\\\")\")))\n\n;; Eshell Syntax Highlighting\n\n;; (use-package eshell-syntax-highlighting\n;;   :after eshell-mode\n;;   :ensure t ;; Install if not already installed.\n;;   :config\n;;   ;; Enable in all Eshell buffers.\n;;   (eshell-syntax-highlighting-global-mode +1))\n\n  ;; Additional package: Toggle eshell window\n(use-package esh-toggle\n  :bind (\"C-x C-z\" . eshell-toggle))\n\n;; capf-autosuggest\n;; As you type, the history auto-suggestion is shown as an overlay after point.\n;; Type [C-e] to insert text from this overlay or [M-f] to only insert a word\n;; from this overlay.  [C-n] is a shorthand for [C-e] followed by [RET]: it\n;; inserts text from the overlay and sends it to the process.\n;; (capf-autosuggest-mode 1)\n;; (add-hook 'comint-mode-hook #'capf-autosuggest-mode)\n;; (add-hook 'eshell-mode-hook #'capf-autosuggest-mode)\n\n;; -------------------------------------